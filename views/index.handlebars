<h1>Products</h1>
    <ul>
        {{#each products}}
            <li>
                <h2>{{this.title}}</h2>
                <p>{{this.description}}</p>
                <p>Price: ${{this.price}}</p>
                <p>Category: {{this.category}}</p>
                <p>Stock: {{this.stock}}</p>
                <div>
                    {{#each this.thumbnails}}
                        <img src="{{this}}" alt="Thumbnail" width="100" />
                    {{/each}}
                </div>
            </li>
        {{/each}}
    </ul>